# Colors
# Reset
#COLOR_OFF='\e[0m'       # Text Reset

# https://misc.flogisoft.com/bash/tip_colors_and_formatting

# Regular Colors

color ()
{
  echo -e "\e[38;5;$1"'m'
}

BLUE=$(color 50)
GREEN=$(color 49)
GRAY=$(color 246)
CYAN=$(color 6)
PURPLE=$(color 176)
RED=$(color 88)

TIME_COLOR=$GREEN
DASH_COLOR=$GRAY
USERNAME_COLOR=$GREEN
PATH_COLOR=$PURPLE
GIT_COLOR=$BLUE      
INPUT_COLOR=$PURPLE
FAIL_COLOR=$RED
